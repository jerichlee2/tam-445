      "text/latex": [
       "$\\displaystyle - c e^{- 2 c t + 2 x_{3}}$"
      ],
      "text/plain": [
       "-c*exp(-2*c*t + 2*x3)"
      ]
     },
     "execution_count": 7,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "from sympy import *\n",
    "\n",
    "x3, t, c, w = symbols('x3, t, c, w')\n",
    "\n",
    "v = Matrix([[exp(x3-c*t)*cos(w*t)],[exp(x3-c*t)*sin(w*t)], [c]])\n",
    "a = diff(v, t)\n",
    "\n",
    "simplify(a.dot(v))"
   ]
